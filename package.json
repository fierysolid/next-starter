{
  "name": "next-start",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build-ts": "tsc -p tsconfig.next.json && tsc -p tsconfig.express.json",
    "build-static": "npx copyfiles -u 1 \"src/static/**/*\" dist",
    "build-next": "next build ./dist",
    "build": "npm run build-ts && npm run build-static && npm run build-next",
    "start-node": "cd ./dist && node server/app.js",
    "start-nodemon": "cd ./dist && nodemon server/app.js --watch server",
    "start": "npm run build-ts && npm run build-static && npm run start-node",
    "dev": "npm run build-ts && npm run build-static && npm run start-nodemon",
    "now": "npm run build && now ./dist && now alias",
    "tslint": "tslint -c tslint.json -p tsconfig.next.json && tslint -c tslint.json -p tsconfig.server.json",
    "preexport": "npm run build",
    "export": "next export ./dist",
    "preserve-static": "npm run export",
    "serve-static": "serve ./dist/out"
  },
  "dependencies": {
    "@segment/snippet": "4.4.0",
    "@types/next-seo": "1.10.0",
    "@types/segment-analytics": "0.0.30",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.11",
    "apollo-link-context": "1.0.17",
    "apollo-link-error": "1.1.10",
    "apollo-link-http": "1.5.14",
    "apollo-link-rest": "0.7.3",
    "apollo-link-retry": "2.2.13",
    "apollo-utilities": "1.2.1",
    "babel-plugin-inline-react-svg": "1.1.0",
    "cacheable-response": "1.5.2",
    "express": "4.16.4",
    "graphql": "14.2.1",
    "graphql-anywhere": "4.2.1",
    "graphql-tag": "2.10.1",
    "isomorphic-unfetch": "3.0.0",
    "jwt-decode": "2.2.0",
    "moment": "2.24.0",
    "next": "8.1.0",
    "next-images": "1.1.1",
    "next-seo": "1.11.2",
    "node-fetch": "2.5.0",
    "node-sass": "4.12.0",
    "react": "16.8.6",
    "react-apollo": "2.5.5",
    "react-dom": "16.8.6",
    "serve": "11.0.0",
    "sitemap": "2.1.0",
    "styled-jsx-plugin-sass": "1.0.0"
  },
  "devDependencies": {
    "@types/express": "4.16.1",
    "@types/next": "8.0.5",
    "@types/react": "16.8.15",
    "@zeit/next-typescript": "1.1.1",
    "nodemon": "1.19.0",
    "tslint": "5.16.0",
    "tslint-config-standard": "8.0.1",
    "tslint-loader": "3.5.4",
    "tslint-react": "4.0.0",
    "typescript": "3.4.5"
  }
}
